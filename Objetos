//JavaScript
//Objetos

const pessoa = {
nome: "Jose",
idade: 22,
ehMaiordeIdade: true
altura: 1.64
};

//Verificar se pode andar no parque
if(pessoa.idade > 12){
  if(pessoa.altura > 1.40){
    console.log(pessoa.nome + " Pode entrar no parque");
  }else{
    console.log(pessoa.nome + "Não tem altura mínima");
  }
  }else{
    console.log(pessoa.nome + " Não tem idade mínima");
  }

//verificar se é maior de idade
if (pessoa.idade < 18){
  console.log(pessoa.nome + " É menor de idade");
}else if (pessoa.idade < 60){
  console.log(pessoa.nome + " é adulta");
}else{
  console.log(pessoa.nome + " é idosa"):
}
