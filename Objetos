//JavaScript
//Objetos

const pessoa = {
nome: "Jose",
idade: 22,
ehMaiordeIdade: true
altura: 1.64
};

if (pessoa.idade < 18){
  console.log(pessoa.nome + " É menor de idade");
}else if (pessoa.idade < 60){
  console.log(pessoa.nome + " é adulta");
}else{
  console.log(pessoa.nome + " é idosa"):
}
